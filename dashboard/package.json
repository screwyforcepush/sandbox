{
  "name": "devpod-dashboard",
  "version": "1.0.0",
  "description": "Web dashboard for monitoring and managing DevPod workspaces",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "npm run build:frontend",
    "build:frontend": "cd frontend && npm run build",
    "install:all": "npm install && cd frontend && npm install"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "playwright": "^1.54.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "devpod",
    "dashboard",
    "monitoring",
    "containers"
  ],
  "author": "Claude CAES",
  "license": "MIT"
}
